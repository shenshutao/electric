<div id="wrapper">
    <%= render "shared/NavMenu" %>

        <div id="page-wrapper">
            <div style="height: 30px;"></div>
            <div class="row">
                <div class="col-lg-8">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <i class="fa fa-bar-chart-o fa-fw"></i> Weekly Consume
                        </div>
                        <!-- /.panel-heading -->
                        <div class="panel-body">
                            <div id="week-goal-chart"></div>
                        </div>
                        <!-- /.panel-body -->
                    </div>

                </div>
                <!-- /.col-lg-8 -->
                <div class="col-lg-4">
